#!/usr/bin/make -f
# -*- makefile -*-

build:
	echo noop

binary:
	curl -sSL --fail --retry 5 https://github.com/containernetworking/cni/releases/download/v{{ .Version }}/cni-{{ .Arch }}-v{{ .Version }}.tgz | tar -xz
	dh_testroot
	dh_auto_install
	dh_shlibdeps
	dh_install
	dh_installdeb
	dh_gencontrol
	dh_md5sums
	dh_builddeb

%:
	dh $@
